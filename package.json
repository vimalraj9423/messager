{
  "name": "example-node-server",
  "version": "1.0.0",
  "description": "Example Node Server w/ Babel",
  "main": "lib/index.js",
  "scripts": {
    "start": "nodemon --watch dist --delay 1 dist/server.js",
    "start:pm": "pm2 start ecosystem.config.js && pm2 logs STATIC",
    "build": "babel lib -d dist --watch",
    "serve": "node dist/server",
    "clean:dist": "rimraf ./dist/*",
    "live-server": "live-server --port=4000 --host=localhost --watch=./dist/",
    "start:dev": "parallelshell  \"npm run start -s\" \"npm run build -s\"",
    "pm:dev": "parallelshell  \"npm run start:pm -s\" \"npm run build -s\"",
    "test": "mocha --compilers js:babel-register"
  },
  "dependencies": {
    "compression": "^1.6.2",
    "express": "^4.15.3",
    "express-dynamic-router": "^0.2.7",
    "express-generator": "^4.15.0",
    "material-ui": "^0.18.3",
    "material-ui-image": "^1.3.1",
    "mysql": "^2.13.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-tap-event-plugin": "^2.0.1"
  },
  "author": "James Kyle <me@thejameskyle.com>",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.11.0",
    "babel-register": "^6.11.6",
    "live-reload": "^1.1.0",
    "mocha": "^3.0.1",
    "nodemon": "^1.11.0",
    "rimraf": "^2.6.1"
  }
}
