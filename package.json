{
  "name": "example-node-server",
  "version": "1.0.0",
  "description": "Example Node Server w/ Babel",
  "main": "lib/index.js",
  "scripts": {
    "start": "nodemon --watch dist --delay 1 dist/server.js",
    "start:pm": "pm2 start ecosystem.config.js && pm2 logs APP",
    "start:dev": "parallelshell \"npm run build -s\" \"npm run start -s\"",
    "start:pm:dev": "parallelshell \"npm run start:pm -s\" \"npm run build -s\"",
    "build": "parallelshell \"npm run build:server -s\" \"npm run build:client -s\"",
    "build:server": "babel lib -d dist --watch",
    "build:client": "webpack --watch",
    "build:client:prod": "webpack -p --config webpack.prod.config.js --display-optimization-bailout",
    "serve": "node dist/server",
    "clean:dist": "rimraf ./dist/*",
    "clean:swap": "find ./ -type f -name \"\\.*sw[klmnop]\" -delete",
    "format": "prettier --write  --single-quote --no-semi \"./lib/**/*.js\" \"./lib/*.js\"",
    "live-server": "live-server --port=4000 --host=localhost --watch=./dist/",
    "lint": "eslint \"./lib/**/*.js\"",
    "lint:fix": "eslint --fix \"./lib/**/*.js\"",
    "fix": "yarn run lint:fix && yarn run format",
    "lint-staged": "lint-staged",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "compression": "^1.6.2",
    "express": "^4.15.3",
    "express-dynamic-router": "^0.2.7",
    "express-generator": "^4.15.0",
    "json-loader": "^0.5.4",
    "material-auto-rotating-carousel": "^1.4.1",
    "material-ui": "next",
    "material-ui-icons": "^1.0.0-alpha.19",
    "material-ui-image": "^1.3.1",
    "mysql": "^2.13.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-motion": "^0.5.0",
    "react-swipeable-views": "^0.12.3",
    "react-tap-event-plugin": "^2.0.1",
    "styled-components": "^2.1.0",
    "webpack-dev-server": "^2.5.0"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.11.0",
    "babel-register": "^6.11.6",
    "css-loader": "^0.28.4",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-prettier": "^2.1.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.0.0",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^0.13.4",
    "jest": "^20.0.4",
    "lint-staged": "^4.0.0",
    "live-reload": "^1.1.0",
    "live-server": "^1.2.0",
    "mocha": "^3.0.1",
    "nodemon": "^1.11.0",
    "parallelshell": "^3.0.1",
    "pm2": "^2.5.0",
    "prettier": "^1.5.0",
    "rimraf": "^2.6.1",
    "webpack": "^3.0.0",
    "webpack-bundle-analyzer": "^2.8.2"
  },
  "lint-staged": {
    "lib/**/*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "roots": [
      "dist/__tests__/"
    ]
  }
}
